{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Urls": "http://*:5000",
  "jwt": {
    "authority": "https://gitlab.example.com",
    "issuer": "gitlab.example.com",
    "validate": {
      "audience": false
    },
    "debug": false
  },
  "gitlab": {
    "hostname": "https://gitlab.example.com",
    "apiKey": "your-api-key",
    "sudoUserLogin": "your-service-user",
    "tokenConfig" : {
      "scopes": [
        "write_repository"
      ],
      "revokeSeconds": 60
    }
  }
}
